{
  "name": "iobroker.google-spreadsheet",
  "version": "0.2.0",
  "description": "Append data to google spreadsheets",
  "author": {
    "name": "Thomas Pohl",
    "email": "post@thomaspohl.net"
  },
  "homepage": "https://github.com/ThomasPohl/ioBroker.google-spreadsheet",
  "license": "Apache-2.0",
  "keywords": [
    "google",
    "spreadsheet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomasPohl/ioBroker.google-spreadsheet.git"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.0.4",
    "@types/gapi": "^0.0.47",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.6.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.6.0",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/testing": "^4.1.0",
    "@tsconfig/node14": "^14.1.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.3.3",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.13.1",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.50.0",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "build-adapter ts",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/ThomasPohl/ioBroker.google-spreadsheet/issues"
  },
  "readmeFilename": "README.md"
}
